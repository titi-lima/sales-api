{
  "name": "sales-server",
  "version": "1.0.0",
  "main": "dist/server.js",
  "license": "MIT",
  "engines": {
    "node": ">=18.16"
  },
  "dependencies": {
    "@anatine/zod-openapi": "^2.2.6",
    "@aws-sdk/client-s3": "^3.650.0",
    "@aws-sdk/s3-request-presigner": "^3.649.0",
    "@prisma/client": "5.19.1",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.23",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-winston": "^4.2.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "1.4.5-lts.1",
    "prisma": "^5.19.1",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.14.2",
    "zod": "^3.23.8"
  },
  "scripts": {
    "build": "swc src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "start": "npm run build && node dist/src/server.js",
    "dev": "concurrently \"ts-node-dev --poll --exit-child -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src/server.ts\" \"prisma generate --watch\"",
    "prod": "npm run migration:deploy && npm run start",
    "migration": "cross-env DATABASE_HOST='localhost' prisma migrate dev",
    "migration:deploy": "prisma migrate deploy",
    "migration:reset": "cross-env DATABASE_HOST='localhost' prisma migrate reset --force",
    "studio": "cross-env DATABASE_HOST='localhost' prisma studio",
    "generate": "prisma generate",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "push": "cross-env DATABASE_HOST='localhost' prisma db push",
    "test:ci": "cross-env DATABASE_URL='postgres://postgres:postgres@localhost:5432/postgres' prisma db push && cross-env DATABASE_URL='postgres://postgres:postgres@localhost:5432/postgres' JWT_ACCESS_SECRET=secret jest --runInBand --ci",
    "test": "cross-env DATABASE_URL='postgres://postgres:docker@localhost:5433/sales-test' prisma db push && cross-env DATABASE_URL='postgres://postgres:docker@localhost:5433/sales-test' jest --runInBand",
    "typecheck": "tsc --noEmit --skipLibCheck"
  },
  "devDependencies": {
    "@swc/jest": "^0.2.36",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.12",
    "@types/node": "22.5.4",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-security": "^3.0.1",
    "jest": "^29.7.0",
    "openapi-types": "^12.1.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-jest-resolver": "^2.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
  }
}
